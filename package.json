{
  "name": "location-based-service-search",
  "version": "1.0.0",
  "description": "A system to search for services based on location",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "prisma generate && ts-node-dev --respawn src/app.ts",
    "build": "prisma generate && tsc",
    "test": "jest --runInBand --detectOpenHandles",
    "test:setup": "jest --runInBand --detectOpenHandles --setupFiles ./src/tests/setup.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "express": "^4.18.2",
    "http-status-codes": "^2.3.0",
    "mysql2": "^3.6.0",
    "prisma": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
